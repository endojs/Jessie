{
  "name": "jessie.js",
  "version": "0.2.0",
  "description": "Jessie standard library",
  "type": "module",
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/Jessie/tree/master/packages/lib#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/Jessie.git"
  },
  "bugs": {
    "url": "https://github.com/endojs/Jessie/issues"
  },
  "main": "./src/main.js",
  "types": "./types/main.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/main.js"
  },
  "scripts": {
    "build": "yarn build:types",
    "build:types": "tsc src/*.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "cover": "nyc ava",
    "lint": "eslint '**/*.js'",
    "lint-fix": "eslint --fix '**/*.js'",
    "prepublish": "yarn clean && yarn build",
    "test": "ava"
  },
  "dependencies": {
    "@endo/far": "^0.2.3"
  },
  "devDependencies": {
    "@endo/ses-ava": "^0.2.25",
    "@agoric/swingset-vat": "^0.26.0",
    "ava": "^3.12.1",
    "eslint": "^7.23.0",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "ses": "^0.15.15",
    "typescript": "^4.6.4"
  },
  "files": [
    "LICENSE*",
    "src",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "@endo",
      "plugin:@jessie.js/recommended"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "esm"
    ],
    "timeout": "2m"
  }
}
