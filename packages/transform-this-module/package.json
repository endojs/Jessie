{
  "name": "@jessie.js/transform-this-module",
  "version": "0.2.2",
  "description": "Indicate that the importer needs rewriting to be valid Jessie.",
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": "https://github.com/endojs/Jessie/tree/main/packages/transform-this-module#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/Jessie.git"
  },
  "bugs": {
    "url": "https://github.com/endojs/Jessie/issues"
  },
  "type": "module",
  "main": "./src/main.js",
  "types": "./types/main.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/main.js"
  },
  "scripts": {
    "build": "yarn build:types",
    "build:types": "tsc src/*.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "clean": "rm -rf types",
    "cover": "nyc ava",
    "lint": "eslint '**/*.js'",
    "lint-fix": "eslint --fix '**/*.js'",
    "prepublish": "yarn clean && yarn build",
    "test": "exit 0"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "eslint": "^7.23.0",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "typescript": "^4.6.4"
  },
  "files": [
    "LICENSE*",
    "src",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "@endo",
      "plugin:@jessie.js/recommended"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "2m"
  }
}
